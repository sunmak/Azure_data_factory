{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sunfactory"
		},
		"Snowflake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Snowflake'"
		},
		"storedir_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storedir'"
		},
		"adflog2_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'adflog2'"
		},
		"storedir_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storedir.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sunfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Snowflake_connectionString": {
						"value": "[parameters('Snowflake_connectionString')]"
					},
					"storedir_accountKey": {
						"value": "[parameters('storedir_accountKey')]"
					},
					"adflog2_sasUri": {
						"value": "[parameters('adflog2_sasUri')]"
					},
					"storedir_properties_typeProperties_url": {
						"value": "[parameters('storedir_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}